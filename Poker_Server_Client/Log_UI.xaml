<Window x:Class="Poker_Server_Client.Log_UI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Log_UI" Height="300" Width="300">
    <Grid>
        <Button x:Name="Login_Btm" Content="Login" HorizontalAlignment="Left" Margin="42,172,0,0" VerticalAlignment="Top" Width="75" Click="Login_Btm_Click"/>
        <Button x:Name="Register_Btm" Content="Register" HorizontalAlignment="Left" Margin="180,172,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox x:Name="Account_Box" HorizontalAlignment="Left" Height="23" Margin="55,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200"/>
        <TextBox x:Name="Password_Box" HorizontalAlignment="Left" Height="23" Margin="55,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200"/>

    </Grid>
</Window>
